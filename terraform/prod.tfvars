# Prefix
prefix = "giit"

# Environments
env = "prod"
# Resource Group arguments
rg_name     = "rg"
rg_location = "Central India"

# Network Security Group arguments
nsg_name = "nsg"

# Virtual Network arguments
vnet_name          = "vnet"
vnet_address_space = ["10.0.0.0/16", "10.1.0.0/16"]

# Subnet arguments
subnet_name             = "subnet"
subnet_address_prefixes = ["192.168.0.0/24"]

# vm arguments
vm_size        = "Standard_DS1_V2"
admin_username = "produser"

